{
  "plugins": [
    "react"
  ],
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true
  },
  "parser": "babel-eslint",
  "rules": {
    "import/first": 0,
    "max-len": [
      1,
      {
        "code": 200
      }
    ],
    "no-unused-vars": 1,
    "react/jsx-uses-vars": 2,
    "global-require": 0,
    "prefer-destructuring": 0,
    "class-methods-use-this": 0,
    "react/no-unused-state": 1,
    "react/prop-types": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "no-console": 0
  }
}