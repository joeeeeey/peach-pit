{
  "name": "peach-pit-space-web",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "antd": "^3.4.1",
    "axios": "^0.18.0",
    "immutable": "^4.0.0-rc.12",
    "js-cookie": "^2.2.0",
    "material-ui": "1.0.0-beta.40",
    "material-ui-icons": "^1.0.0-beta.36",
    "md5": "^2.2.1",
    "prop-types": "15.6.1",
    "react": "16.3.1",
    "react-color": "^2.14.1",
    "react-dom": "16.3.1",
    "react-grid-layout": "^0.16.6",
    "react-loadable": "^5.3.1",
    "react-measure": "^2.0.2",
    "react-quill": "^1.2.7",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-scroll": "^1.7.9",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "0.0.54"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run delete-maps",
    "delete-maps": "npm run delete-map-files && npm run delete-references-to-map-files",
    "delete-map-files": "find ./build -name '*.map' -delete",
    "delete-references-to-map-files": "find ./build -regex '.*\\.\\(js\\|css\\)' -exec sed -i -E '\\/[\\*\\/]#\\ssourceMappingURL=main(\\.[0-9a-f]+)?\\.(css|js)\\.map(\\*\\/)?/g' {} +",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": {
    "/api": {
      "target": "http://127.0.0.1:7001",
      "pathRewrite": {
        "^/api": ""
      }
    }
  },
  "devDependencies": {
    "babel-plugin-import": "^1.7.0",
    "eslint-plugin-react": "^7.12.4"
  }
}
